       public static TipoInfo PideDatosprofesor(int c) {
        TipoInfo Datos = new TipoInfo();
        Scanner teclado = new Scanner(System.in);
        System.out.println("INTRODUZCA LOS DATOS");
        System.out.print("-Nombre: ");
        Datos.setTermino(teclado.nextLine());
        System.out.print("- No Empleado: ");
        Datos.setSignificado(teclado.nextLine());
        System.out.print("-asignatura a impartir : ");
        Datos.setRegistro(teclado.nextLine());
        {
        Datos.setcategoria(c3);
        
        }
       
        
        return Datos;
    }
          
        
        
   
     
       
       
    public static TipoInfo PideDatos(int c) {
        TipoInfo Datos = new TipoInfo();
        Scanner teclado = new Scanner(System.in);
        System.out.println("INTRODUZCA LOS DATOS");
        System.out.print("-Nombre: ");
        Datos.setTermino(teclado.nextLine());
        System.out.print("-ID: ");
        Datos.setSignificado(teclado.nextLine());
        System.out.print("-registro: ");
        Datos.setRegistro(teclado.nextLine());
        if(c==0){
        
        Datos.setcategoria(c0);
        
        }
        if(c==1){
        
        Datos.setcategoria(c1);
        
        }
         if(c==2){
        
        Datos.setcategoria(c2);
        
        }
          if(c==3){
        
        Datos.setcategoria(c3);
        
        }
        
        
        return Datos;
    }

    //otros métodos
    public final static void agregarLineas(int n) {
        for (int i = 0; i < n; i++) {
            System.out.println();
        }
    }

    public final static void pausa() {
        System.out.print("Press Any Key To Continue...");
        new java.util.Scanner(System.in).nextLine();
    }

    public static String PideCadena(String pTexto) {
        String cadena;
        Scanner teclado = new Scanner(System.in);
        System.out.print(pTexto);
        cadena = teclado.nextLine();
        return cadena;
    }

    public static int PideEntero(String pTexto) {
        int numero;
        Scanner teclado = new Scanner(System.in);
        System.out.print(pTexto);
        numero = teclado.nextInt();
        
        return numero;
    }

   
     
    public static void cargar (TadListaSimple T,String c){
        TipoInfo temp;
        switch(c){
            case "Algoritmos":
                for(int i=0;i<5;i++){
                    temp = new TipoInfo();
                    switch(i){
                        case 0:
                            temp.setTermino(Marcoss);
                        temp.setSignificado(MarcosT);
                        temp.setcategoria(c2);
                       
                            break;
                         case 1:temp.setTermino(ArchivoT);
                        temp.setSignificado(ArchivoS);
                        temp.setcategoria(c2);
                       
                            break;
                             case 2:temp.setTermino(ByteT);
                        temp.setSignificado(ByteS);
                        temp.setcategoria(c2);
                       
                            break;
                             case 3:temp.setTermino(CarpetaT);
                        temp.setSignificado(CarpetaS);
                        temp.setcategoria(c2);
                      
                            break;
                             case 4:temp.setTermino(BinarioT);
                        temp.setSignificado(BinarioS);
                        temp.setcategoria(c2);
                       
                            break;
                    
                    }
                    
                 T.InsertarFinal(temp);
                
                }
                break;
                
            case "Bases de Datos":
                for(int i=0;i<3;i++){
                    temp = new TipoInfo();
                    if(i==0){
                        temp.setTermino(BugT);
                        temp.setSignificado(BugS);
                        temp.setcategoria(c3);
                       
                    }
                    if(i==1){
                        temp.setTermino(CDPROMT);
                        temp.setSignificado(CDPROMS);
                        temp.setcategoria(c3);
                        
                    }
                    if(i==2){
                        temp.setTermino(AlgoritmodeCifradoT);
                        temp.setSignificado(AlgoritmodeCifradoS);
                        temp.setcategoria(c3);
                       
                    }
                   
                    
                 T.InsertarFinal(temp);
                
                }
                break;
           
            case "Redes Informaticas":
                for(int i=0;i<3;i++){
                    temp = new TipoInfo();
                    if(i==0){
                        temp.setTermino(BandaAnchaS);
                        temp.setSignificado(BandaAncha);
                      
                    }
                    if(i==1){
                        temp.setTermino(CachéT);
                        temp.setSignificado(Caché);
                        
                        
                    }
                    if(i==2){
                        temp.setTermino(AntivirusT);
                        temp.setSignificado(Antivirus);
                     
                       
                    }
                   
                    
                 T.InsertarFinal(temp);
                
                }
                break;
            case "Informatica General":
                for(int i=0;i<3;i++){
                    temp = new TipoInfo();
                    if(i==0){
                        temp.setTermino(Appletst);
                        temp.setSignificado(Applets);
                        temp.setcategoria(c0);
                       
                    }
                    if(i==1){
                        temp.setTermino(CDRT);
                        temp.setSignificado(CDRS);
                        temp.setcategoria(c0);
                        
                    }
                    if(i==2){
                        temp.setTermino(BIOST);
                        temp.setSignificado(BIOSS);
                        temp.setcategoria(c0);
                       
                    }
                   
                    
                 T.InsertarFinal(temp);
                
                }
                break;
        
                case "Programacion":
                for(int i=0;i<3;i++){
                    temp = new TipoInfo();
                    if(i==0){
                        temp.setTermino(BITT);
                        temp.setSignificado(BITS);
                        temp.setcategoria(c1);
                       
                    }
                    if(i==1){
                        temp.setTermino(ASCIIT);
                        temp.setSignificado(ASCIIS);
                        temp.setcategoria(c1);
                        
                    }
                    if(i==2){
                        temp.setTermino(CDWT);
                        temp.setSignificado(CDWS);
                        temp.setcategoria(c1);
                       
                    }
                   
                    
                 T.InsertarFinal(temp);
                
                }
                break;
        
        
        }
    
    
    }
    
    
